{
    "name": "zenstruck/queue",
    "type": "library",
    "description": "Unified API for different queue services.",
    "keywords": ["queue", "amazon sqs", "beanstalkd", "redis"],
    "homepage": "http://zenstruck.com/projects/Queue",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/event-dispatcher": "~2.3"
    },
    "require-dev": {
        "psr/log": "~1.0",
        "phpspec/phpspec": "~2.0",
        "phpunit/phpunit": "~4.0",
        "aws/aws-sdk-php": "~2.6",
        "predis/predis": "~0.8",
        "pda/pheanstalk": "~2.0"
    },
    "autoload": {
        "psr-4": { "Zenstruck\\Queue\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Zenstruck\\Queue\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
